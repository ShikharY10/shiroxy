{
    "default": {
        "mode": "http",
        "logpath": "/home/shikharcode/Main/opensource/shiroxy",
        "datapersistancepath": "/home/shikharcode/Main/opensource/shiroxy",
        "enablednschallengesolver": "",
        "timeout": {
            "connect": "10s",
            "client": "10s",
            "server": "10s"
        },
        "storage": {
            "location": "memory"
        },
        "analytics": {
            "collectioninterval": 10,
            "routename": "/analytics"
        },
        "errorresponses": {
            "errorpagebuttonname": "Comeata",
            "errorpagebuttonurl": "https://youtube.com"
        }
    },
    "frontend": {
        "backend": "shiroxy-test",
        "bind": {
            "port": "8080",
            "host": "",
            "secure": {
                "target": "single",
                "singletartgetmode": "certandkey/shiroxysignlesecure",
                "certandkey": {
                    "cert": "cert-location",
                    "key": "key-location"
                },
                "shiroxysignlesecure": {
                    "domain": "shikharcode.com"
                }
            }
        },
        "options": [
            "http-server-close",
            "forwardfor"
        ],
        "secure": false,
        "secureverify": "required",
        "defaultbackend": "",
        "fallbackbackend": "",
        "balance": "round-robin"
    },
    "backend": {
        "name": "shiroxy-test",
        "servers": [
            {
                "id": "<backend-name-server-1>",
                "host": "",
                "port": "8001",
                "healthurl": ""
            },
            {
                "id": "<backend-name-server-1>",
                "host": "",
                "port": "8002",
                "healthurl": ""
            }
        ],
        "healthcheckmode": "home/url",
        "healthchecktriggerduration": 5
    },
    "logging": {
        "enable": true,
        "mode": "native/http/native-http",
        "schema": [
            "[date-time] "
        ],
        "include": [
            "*",
            ""
        ]
    },
    "webhook": [
        {
            "enable": true,
            "events": [
                ""
            ],
            "url": ""
        }
    ],
    "health": {
        "enable": true,
        "bind": {
            "host": "*",
            "port": ""
        },
        "auth": {
            "user": "",
            "password": ""
        }
    }
}